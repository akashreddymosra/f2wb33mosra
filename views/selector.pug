extends layout

block content
  h1= title
  - var image_names = ["pic1", "pic2", "pic3", "pic4", "pic5"]
  - for(let i = 0; i < 3; i++) {
    - let number = Math.floor(Math.random() * 5)
        img(src=`/images/${image_names[number]}.jpg`, alt="different images every time", height="300px", width="300px")
  - }